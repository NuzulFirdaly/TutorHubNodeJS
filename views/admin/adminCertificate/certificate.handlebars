                                        <div class="col-lg-6">
                                            <div class="px-4 py-3">
                                                <div class="text-center">
                                                    <img src="/admin/images/{{document}}" class="img-fluid">
                                                </div>
                                                <div class="text pt-3 w-100 text-center">
                                                    <h3>{{name}}</h3>
                                                    <span>{{document}}</span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-lg-6">
                                            <div class="px-4 py-3">
                                                <div class="text-center">
                                                    <img src="/admin/images/{{document}}" class="img-fluid">
                                                </div>
                                                <div class="text pt-3 w-100 text-center">
                                                    <h3>{{name}}</h3>
                                                    <span>{{document}}</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div> --}}
                                    <div>
                                        <div class="px-4 py-3">
                                            <div class="text-center">
                                                <img src="/admin/images/{{document}}" class="img-fluid">
                                            </div>
                                            <div class="text pt-3 w-100 text-center">
                                                <h3>{{name}}</h3>
                                                <span>{{document}}</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                    <button type="submit" id="{{pending_institution_id}}"
                                        class="inst_reject btn btn-primary">Reject</button>
                                    <button type="submit" id="{{pending_institution_id}}"
                                        class="inst_approve btn btn-primary">Approve</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Modal -->
                    {{/each}}
                </div>
            </div>
        </div>
    </section>
</div>