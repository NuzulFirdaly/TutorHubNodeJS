<div class="container bg-white shadow rounded overflow-hidden"
    style="width: 85%; padding-top: 50px; margin-top: 120px; margin-bottom: 50px; padding-left: 50px; padding-right: 50px;">

    <h2 class="display-4">Tutor Certificate</h2>

    <section class="ftco-section">

        <table id="tutOverview" class="table display responsive nowrap" style="width:100%">
            <thead>
                <tr>
                    <th>Ticket ID</th>
                    <th>Occupation</th>
                    <th>From</th>
                    <th>To</th>
                    <th>College Country</th>
                    <th>College Name</th>
                    <th>Major</th>
                    <th>Graduate</th>
                    <th>Date of Birth</th>
                    <th>NRIC</th>
                    <th>Certificate</th>
                    <th>ID</th>
                </tr>
            </thead>
            <tbody>
                {{#each pTutList}}
                <tr>
                    <td>{{pending_ticket_id}}</td>
                    <td>{{occupation}}</td>
                    <td>{{fromyear}}</td>
                    <td>{{toyear}}</td>
                    <td>{{college_country}}</td>
                    <td>{{college_name}}</td>
                    <td>{{major}}</td>
                    <td>{{graduate}}</td>
                    <td>{{dateofbirth}}</td>
                    <td>{{NRIC}}</td>
                    <td>{{cert}}</td>
                    <td>{{userUserId}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>

        <br><br><br><br><br>
        <h2>Review</h2>

        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <h2 class="heading-section mb-5 pb-md-4">Tutor Carousel</h2>
                </div>
                <div class="col-md-12">
                    <div class="featured-carousel owl-carousel">
                        {{#each pTutList}}
                        <div class="item" id="tut_{{cert}}" data-hash="tut_{{cert}}">
                            <div class="work">
                                <div class="img d-flex align-items-center justify-content-center rounded"
                                    style="background-image: url(/admin/images/{{cert}});">
                                    <a class="icon d-flex align-items-center justify-content-center" data-toggle="modal"
                                        data-target="#tut_{{pending_ticket_id}}">
                                        <span class="ion-ios-search"></span>
                                    </a>
                                </div>
                                <div class="text pt-3 w-100 text-center">
                                    <h3><a href="#" onclick=findInOverview2("{{pending_ticket_id}}")>{{userUserId}}</a>
                                    </h3>
                                    <span>{{cert}}</span>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                    {{#each pTutList}}
                    <!-- Modal -->
                    <div class="modal fade" id="tut_{{pending_ticket_id}}" tabindex="-1" role="dialog"
                        aria-labelledby="ModalTitle" aria-hidden="true">
                        <div class="modal-xl modal-dialog modal-dialog-scrollable modal-dialog-centered"
                            role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h3 class="modal-title" id="ModalTitle">{{userUserId}}</h3>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"
                                        style="margin: auto; margin-right: 0px; margin-top: -10px;">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div class="modal-body">
                                    {{!-- <div class="row">
                                        <div class="col-lg-6">
                                            <div class="px-4 py-3">
                                                <div class="text-center">
                                                    <img src="/admin/images/{{cert}}" class="img-fluid">
                                                </div>
                                                <div class="text pt-3 w-100 text-center">
                                                    <h3>{{userUserId}}</h3>
                                                    <span>{{cert}}</span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-lg-6">
                                            <div class="px-4 py-3">
                                                <div class="text-center">
                                                    <img src="/admin/images/{{cert}}" class="img-fluid">
                                                </div>
                                                <div class="text pt-3 w-100 text-center">
                                                    <h3>{{userUserId}}</h3>
                                                    <span>{{cert}}</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div> --}}
                                    <div class="px-4 py-3">
                                        <div class="text-center">
                                            <img src="/admin/images/{{cert}}" class="img-fluid">
                                        </div>
                                        <div class="text pt-3 w-100 text-center">
                                            <h3>{{userUserId}}</h3>
                                            <span>{{cert}}</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                    <button type="submit" id="{{pending_ticket_id}}"
                                        class="tut_reject btn btn-primary">Reject</button>
                                    <button type="submit" id="{{pending_ticket_id}}"
                                        class="tut_approve btn btn-primary">Approve</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Modal -->
                    {{/each}}
                </div>
            </div>
        </div>
    </section>

    <h2 id="institution" class="display-4">Institution Certificate</h2>

    <section class="ftco-section">

        <h2 class="heading-section mb-5 pb-md-4 text-md-left" style="text-align: center;" id="toOverviewInst">Overview
        </h2>

        <table id="instOverview" class="table display responsive nowrap" style="width:100%">
            <thead>
                <tr>
                    <th>Ticket ID</th>
                    <th>Name</th>
                    <th>Address</th>
                    <th>Postal Code</th>
                    <th>Email</th>
                    <th>Website</th>
                    <th>Office Number</th>
                    <th>Document</th>
                </tr>
            </thead>
            <tbody>
                {{#each pInstList}}
                <tr>
                    <td>{{pending_institution_id}}</td>
                    <td>{{name}}</td>
                    <td>{{address}}</td>
                    <td>{{postalcode}}</td>
                    <td>{{iemail}}</td>
                    <td><a href="{{website}}" target="_blank">{{website}}</a></td>
                    <td>{{officeno}}</td>
                    <td><a href="#inst_{{document}}">{{document}}</a></td>
                </tr>
                {{/each}}
            </tbody>
        </table>

        <br><br><br><br><br>

        <h2 class="heading-section mb-5 pb-md-4 text-md-left" style="text-align: center;">Review</h2>

        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <h2 class="heading-section mb-5 pb-md-4">Institution Certificates</h2>
