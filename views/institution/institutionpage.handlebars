</head>


<!-- ========================= SECTION INTRO ========================= -->
<section class="section-intro">

	{{#ifEquals bannerarray.length 0}}

	<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
		<ol class="carousel-indicators">
			<li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
			<li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
			<li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
		</ol>
		<div class="carousel-inner">
			<div class="carousel-item active">
				<img class="d-block w-100" style="height: 420px" src="/images/banners/1.jpg" alt="First slide">
			</div>
			<div class="carousel-item">
				<img class="d-block w-100" style="height: 420px" src="/images/banners/1.jpg" alt="Second slide">
			</div>
			<div class="carousel-item">
				<img class="d-block w-100" style="height: 420px" src="/images/banners/1.jpg" alt="Third slide">
			</div>
		</div>
		<a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
			<span class="carousel-control-prev-icon" aria-hidden="true"></span>
			<span class="sr-only">Previous</span>
		</a>
		<a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
			<span class="carousel-control-next-icon" aria-hidden="true"></span>
			<span class="sr-only">Next</span>
		</a>
	</div>

	{{else}}

	<div id="carousel1_indicator" class="carousel slide" data-ride="carousel">
		<ol class="carousel-indicators">
			{{#forloop 0 bannerarray.length 1}}
			{{#ifEquals @index 0}}
			<li data-target="#carousel1_indicator" data-slide-to="{{this}}" class="active"></li>
			{{else}}
			<li data-target="#carousel1_indicator" data-slide-to="{{this}}"></li>
			{{/ifEquals}}
			{{!-- <li data-target="#carousel1_indicator" data-slide-to="2"></li> --}}
			{{/forloop}}
		</ol>
		<div class="carousel-inner">
			{{#each bannerarray}}
			{{#ifEquals @index 0}}
			<div class="carousel-item active">
				<img class="d-block w-100 " style="height: 420px"
					src="/images/Institutionpictures/banner/{{bannerpicture}}" alt="slide">
			</div>
			{{else}}
			<div class="carousel-item">
				<img class="d-block w-100 " style="height: 420px"
					src="/images/Institutionpictures/banner/{{bannerpicture}}" alt="slide">
			</div>
			{{/ifEquals}}
			{{!-- <div class="carousel-item">
				<img class="d-block w-100 " style="height: 420px" src="/images/banners/2.jpg" alt="Second slide">
			</div>
			<div class="carousel-item">
				<img class="d-block w-100 " style="height: 420px" src="/images/banners/3.jpg" alt="Third slide">
			</div> --}}
			{{/each}}
		</div>
		{{#if (ifGreater bannerarray.length 1)}}
		<a class="carousel-control-prev" href="#carousel1_indicator" role="button" data-slide="prev">
			<span class="carousel-control-prev-icon" aria-hidden="true"></span>
			<span class="sr-only">Previous</span>
		</a>
		<a class="carousel-control-next" href="#carousel1_indicator" role="button" data-slide="next">
			<span class="carousel-control-next-icon" aria-hidden="true"></span>
			<span class="sr-only">Next</span>
		</a>
		{{/if}}
	</div>

	{{/ifEquals}}
	<!-- ========================= SECTION SPECIAL END// ========================= -->

	<section class="padding-y-sm">
		<div class="container">
			<article class="card card-body">


				<div class="row">
					{{#ifEquals descriptionarray.length 0}}
					<div class="col-md-4">
						<figure class="item-feature">
							<span class="text-primary"><i class="fa fa-2x fa-graduation-cap"></i></span>
							<figcaption class="pt-3">
								<h5 class="title">About Us</h5>
								<p>Something about your institution</p>
							</figcaption>
						</figure> <!-- iconbox // -->
					</div><!-- col // -->
					<div class="col-md-4">
						<figure class="item-feature">
							<span class="text-primary"><i class="fa fa-2x fa-book"></i></span>
							<figcaption class="pt-3">
								<h5 class="title">Courses we offer</h5>
								<p>courses you offer are here!
								</p>
							</figcaption>
						</figure> <!-- iconbox // -->
					</div><!-- col // -->
					{{else}}
					{{#each descriptionarray}}

					<div class="col-md-4">
						<figure class="item-feature">
							<span class="text-primary"><i class="fa fa-2x fa-graduation-cap"></i></span>
							<figcaption class="pt-3">
								<h5 class="title">About Us</h5>
								<p>{{about}}</p>
							</figcaption>
						</figure> <!-- iconbox // -->
					</div><!-- col // -->
					<div class="col-md-4">
						<figure class="item-feature">
							<span class="text-primary"><i class="fa fa-2x fa-book"></i></span>
							<figcaption class="pt-3">
								<h5 class="title">Courses we offer</h5>
								<p>{{mycourse}}
								</p>
							</figcaption>
						</figure> <!-- iconbox // -->
					</div><!-- col // -->
					{{/each}}
					{{/ifEquals}}
					<div class="col-md-4">
						<figure class="item-feature">
							<span class="text-primary"><i class="fa fa-2x fa-users"></i></span>
							<figcaption class="pt-3">
								<h5 class="title">Check out our tutors</h5>
								<p>This will be a link which will be displayed in the main page
								</p>
							</figcaption>
						</figure> <!-- iconbox // -->
					</div> <!-- col // -->
				</div>
			</article>

		</div> <!-- container .//  -->
	</section>

	<!-- social media -->
	<footer class="section-footer">
		<div class="container">
			<br><br>
			<div class="d-flex justify-content-center">
				<h3>Connect with Us!</h3>
			</div>

			<br><br>
			<center>
				<div class="d-flex justify-content-center">
					<ul class="list-inline media-body">
						{{#if widgetarray}}
						{{#each widgetarray}}
						<div class="list-inline-item">
							<a href="{{widgeturl}}" target="_blank"><img class="d-block img-sm rounded-circle"
									src="/images/Institutionpictures/socialmedia/{{widgetimage}}"></a>
						</div>
						{{/each}}
						{{else}}
						<div class="list-inline-item">
							<a href="#"><img class="d-block img-sm rounded-circle"
									src="/images/Institutionpictures/socialmedia/2.jpg"></a>
						</div>
						{{/if}}
					</ul>
				</div>
			</center>
		</div><!-- //container -->
	</footer>
	<!-- end of social media -->
	<br><br><br><br>
	<!-- ========================= SECTION  ========================= -->
	<section>
		<br>
		<h4 class="card-title mb-4"
			style="width: 100%; text-align: center; border-bottom: 1px solid #000; line-height: 0.1em; margin: 10px 0 20px;">
			<span style="background:#fff; padding: 0 50px;">Featured Tutors</span>
		</h4>
		<br><br>
		<!-- tutors here -->
		<center>
			<div class="row  d-flex justify-content-center">
				<div class="d-flex justify-content-center">
					<ul>
						{{#if featuretutorarray}}
						{{#each featuretutorarray}}
						<li class="list-inline-item">
							<img class="img-md rounded-circle mx-auto" src="/images/profilepictures/{{Profile_pic}}">
							<br>
							<h6>{{FirstName}}&nbsp;{{LastName}}</h6>
						</li>
						{{/each}}
						{{else}}
						<li class="list-inline-item">
							<img class="img-md rounded-circle mx-auto" src="/images/profilepictures/avatar2.jpg">
							<br>
							<label>Feature your tutor here!</label>
						</li>
						{{/if}}
					</ul>
				</div>
			</div>
		</center>
		<!-- end of tutors -->
		<br><br><br>
	</section>
	<br><br>
	<section>
		<br>
		<h5 class="card-title mb-4"
			style="width: 100%; text-align: center; border-bottom: 1px solid #000; line-height: 0.1em; margin: 10px 0 20px;">
			<span style="background:#fff; padding: 0 50px;">Featured Courses</span>
		</h5>
		<br><br>
		<!-- Top courses-->
		<div class="row d-flex justify-content-center">
			{{#if allfeaturedcoursearray}}
			{{#each allfeaturedcoursearray}}
			<figure class="card card-product-grid" style="width: 18rem; height: 21rem; margin-right: 20px;">
				<img class="card-img-top" src="/images/coursethumbnails/{{Course_thumbnail}}" height="140" width="240">
				<div class="widget-header">
					<a href="#" class="ml-3 icontext" data-toggle="dropdown" data-offset="20,10">
						<div class="icon"><img class="icon icon-xs rounded-circle"
								src="/images/profilepictures/{{Profile_pic}}"></div>
						<div class="text">
							<small class="text-muted">{{FirstName}} {{LastName}}</small> <br>
						</div>
					</a>
				</div>
				<figcaption class="info-wrap">
					<h4 class="title"><a href="/course/viewcourse/{{course_id}}">{{Title}}</a></h4>
					<p class="text-muted" style="font-size:0.9em;">{{Short_description}}</p>
					<br>
					<h5>{{Hourlyrate}}/Hour</h5>
				</figcaption>
			</figure>
			{{/each}}
			{{else}}
			<figure class="card card-product-grid" style="width: 18rem; height: 21rem; margin-right: 20px;">
				<img class="card-img-top" src="/images/coursethumbnails/avatar2.jpg" height="140" width="240">
				<div class="widget-header">
					<a href="#" class="ml-3 icontext" data-toggle="dropdown" data-offset="20,10">
						<div class="icon"><img class="icon icon-xs rounded-circle" src="/images/profilepictures/2.jpg">
						</div>
						<div class="text">
							<small class="text-muted">Feature your courses here!</small> <br>
						</div>
					</a>
				</div>
				<figcaption class="info-wrap">
					<h4 class="title"><a href="#">CHoose your favourite/top courses</a></h4>
					<p class="text-muted" style="font-size:0.9em; height:58px">Let everyone see it here!</p>
					<br>
					<h5>1/Hour</h5>
				</figcaption>
			</figure> <!-- card // -->
			{{/if}}
		</div>
		<!-- end of top courses-->
		<br><br>
	</section>
	<br>
	<section>
		<h5 class="card-title mb-4"
			style="width: 100%; text-align: center; border-bottom: 1px solid #000; line-height: 0.1em; margin: 10px 0 20px;">
			<span style="background:#fff; padding: 0 50px;">Seminars / Events</span>
		</h5>
		<br><br>
		<!-- seminar / Events-->
		{{#if seminararray}}
		<div class="row d-flex justify-content-center">
			{{#each seminararray}}
			<div class="col-sm-4 mt-3">
				<div class="card">
					<div class="card-body">
						<img class="card-img-top" src="/images/Institutionpictures/seminar/{{SEImage}}"
							style="height: 150px;">
						<hr>
						<h6 class="card-title" style="font-size: medium;">{{SETitle}}</h6>
						<p class="card-text">{{SEDescription}}</p>
						<br>
						<a href="{{SEUrl}}" class="btn btn-primary">Learn more!</a>
					</div>
				</div>
			</div>
			{{/each}}
		</div>
		{{else}}
		<div class="row d-flex justify-content-center">
			<div class="col-sm-4 mt-3">
				<div class="card">
					<div class="card-body">
						<img class="card-img-top" src="/images/Institutionpictures/seminar/TutorHub_logl.png">
						<hr>
						<h6 class="card-title" style="font-size: medium;">Upload your Seminar/events here</h6>
						<p class="card-text"> Let users attending workshops/events you have! let them learn more here.
						</p>
						<br>
						<a href="#" class="btn btn-primary">Learn more!</a>
					</div>
				</div>
			</div>
		</div>
		{{/if}}
		<!-- end of seminar/ events-->
		<br><br>
	</section>
</section>
<!-- ========================= SECTION  END// ========================= -->
