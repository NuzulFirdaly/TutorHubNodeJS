<head>
	<link href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick-theme.min.css" rel="stylesheet" />
	<link href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.css" rel="stylesheet" />
	<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js"></script>
	<script src="js/script.js" type="text/javascript"></script>

	<script type="text/javascript">
		/// some script

		$('.carousel .carousel-item').each(function () {
			var minPerSlide = 3;
			var next = $(this).next();
			if (!next.length) {
				next = $(this).siblings(':first');
			}
			next.children(':first-child').clone().appendTo($(this));

			for (var i = 0; i < minPerSlide; i++) {
				next = next.next();
				if (!next.length) {
					next = $(this).siblings(':first');
				}

				next.children(':first-child').clone().appendTo($(this));
			}
		});


		// jquery ready start
		$(document).ready(function () {
			$('.slick').slick({
				slidesToShow: 2,
				slidesToScroll: 2,
				autoplay: false,
				infinite: true,
				responsive:{
                0:{
                    items:1
                },
                640:{
                    items:3
                },
                1024:{
                    items: 3
                }
            }
			});

			// When the filter values are changed, 
			// apply the filter to slick.
			$('form.filter select').on('change', filter);
			$('#slidename').on("keyup", filter);

			$('form.filter2 select').on('change', filter2);
			$('#slidename2').on("keyup", filter2);


			/**
			 * Add a filter group row.
			 */
			$('.add-filter').on('click', function (event) {
				event.preventDefault()
				$('form.filter .filter-group').first().clone(true).insertBefore($('form.filter .add-filter'));
			});
		});

		function filter() {
			// Declare variabl
			const filter = jQuery("#slidename").val();
			const category = $('.filter-group select').val();

			$('.slick').slick('slickUnfilter');
			$('.slick').slick('slickFilter', function () {
				let content = jQuery(this).find("a").text().toLowerCase();

				return content.indexOf(filter) > -1;

			});
		}
		function filter2() {
			// Declare variabl
			const filter = jQuery("#slidename2").val();
			const category = $('.filter-group select').val();

			$('.slick').slick('slickUnfilter');
			$('.slick').slick('slickFilter', function () {
				let content = jQuery(this).find("a").text().toLowerCase();

				return content.indexOf(filter) > -1;

			});
		}
// jquery end
	</script>
	<style>
		/** Just some styling to make it more readable. */


		.filter {
			margin: 1rem 0;
		}

		.active-filter {
			margin: 1rem 0;
		}

		.slick {
			width: 600px;
			padding-left: 15px;
		}

		.slick .slide {
			width: 200px;
			height: 300px;
			padding-left: 1rem;
			display: flex;
			align-items: center;
			justify-content: center;
			text-align: center;
		}

		.slick-prev:before,
		.slick-next:before {
			color: black;
		}



		#myslides {
			/* Remove default list styling */
			list-style-type: none;
			padding: 15px;
			margin: 0;
		}

		#myslides li a {
			/* Add a border to all links */
			margin-top: 50px;
			/* Prevent double borders */
			/* Grey background color */
			padding: 1px;
			/* Add some padding */
			text-decoration: none;
			/* Remove default text underline */
			font-size: 18px;
			/* Increase the font-size */
			display: block;
		}

		#myslides li a card:hover:not(.header) {
			background-color: #eee;
			/* Add a hover effect to all links, except for headers */
		}
		
	</style>
</head>
